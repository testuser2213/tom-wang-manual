/**
 * Originator
 */
class Role {
        private $state;
        private $name;
        
        public function __construct($name) {
                $this-&gt;state = 100;
                $this-&gt;name = $name;
        }
        
        public function saveState() {
                $memento = new Memento($this-&gt;state);
                Manage::setMemento($memento);
        }
        
        public function recoverState() {
                $memento = Manage::getMemento();
                $this-&gt;state = $memento-&gt;getState();
        }
        
        public function fight() {
                --$this-&gt;state;
        }
        
        public function show() {
                echo &quot;{$this-&gt;name}:{$this-&gt;state}&lt;br/&gt;&quot;;
        }
}

/**
 * Memento
 */
class Memento {
        private $state;
        
        public function __construct($state) {
                $this-&gt;state = $state;
        }
        
        public function getState() {
                return $this-&gt;state;
        }
}

/**
 * Caretaker
 */
class Manage {
        private static $mementos = array();
        
        public static function setMemento($memento) {
                self::$mementos[] = $memento;
        }
        
        public static function getMemento() {
                return array_pop(self::$mementos);
        }
}

