<XPD:ATTR name="Text" type="string">&lt;?php
class Singleton {
        protected static $instance = null;
        
        protected function __construct() {
                //throw new Exception('can not construct!');
        }
        
        protected function __clone() {
                throw new Exception('can not clone!');
        }
        
        public function __wakeup() {
                self::$instance = $this;
        }
        
        public function __destruct() {
                # do some clear job
                # but don't set self::$instance = null
        }
        
        public static function getInstance() {
                if(null === self::$instance) {
                        self::$instance = new self();
                }
                
                return self::$instance;
        }
}

$a = Singleton::getInstance();
# serialize can break singleton, so you'd better use like this
# $a = unserialize(serialize($a));
$b = unserialize(serialize($a));
$c = Singleton::getInstance();
$d = Singleton::getInstance();
var_dump($a === $b); //bool(false);
var_dump($a === $c); //bool(false);
var_dump($b === $c); //bool(true);
var_dump($c === $d); //bool(true);
echo '&lt;br/&gt;';
var_dump($a, $b, $c, $d);
#object(Singleton)#1 (0) { } object(Singleton)#2 (0) { } 
#object(Singleton)#2 (0) { } object(Singleton)#2 (0) { } 
?&gt;
